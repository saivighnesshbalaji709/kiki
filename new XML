<parameters>
   <parameter name="P_FROM_DOC_DATE"         datatype="VARCHAR"/>
   <parameter name="P_FROM_SUPP_NO"        	 datatype="CHARACTER"/>
   <parameter name="P_TO_SUPP_NO"        	 datatype="CHARACTER"/>
   --<parameter name="P_FROM_SUPP_NAME"        datatype="CHARACTER"/>
  -- <parameter name="P_TO_SUPP_NAME"          datatype="CHARACTER"/>
   --<parameter name="P_SUPP_SITE"        	 datatype="CHARACTER"/>
  -- <parameter name="P_SUPP_1"        	 	 datatype="CHARACTER"/>
   --<parameter name="P_SUPP_2"        	 	 datatype="CHARACTER"/>
  -- <parameter name="P_SUPP_3"        	 	 datatype="CHARACTER"/>
  -- <parameter name="P_SUPP_4"        	 	 datatype="CHARACTER"/>
   <parameter name="P_REP_TYPE"        	 	 datatype="CHARACTER"/>
   <parameter name="P_FROM_GL_DATE"          datatype="VARCHAR"/>
   <parameter name="P_TO_GL_DATE"            datatype="VARCHAR"/>
   <parameter name="P_TO_DOC_DATE"           datatype="VARCHAR"/>
   <parameter name="P_CURRENCY"              datatype="CHARACTER"/>
   <parameter name="P_PAY_GROUP"             datatype="CHARACTER"/>
   <parameter name="P_VEND_TYPE"             datatype="CHARACTER"/>
   <parameter name="P_UNVALIDATED_TRX"       datatype="CHARACTER"/>
   <parameter name="P_UNAPPROVED_TRX"        datatype="CHARACTER"/>
   <parameter name="P_ACCOUNTED"             datatype="CHARACTER"/>
</parameters>
<dataQuery>
<sqlStatement name="Q_DATE">
<![CDATA[
	SELECT aps.segment1 SUPP_NO_FROM
		  ,aps1.segment1 SUPP_NO_TO
		 -- , null SUPP_NAME_FROM
		  --, null SUPP_NAME_FROM
		 -- , null SUPP_SITE
		 -- , null SUPP_NO_1
		 -- , null SUPP_NO_2
		 -- , null SUPP_NO_3
		 -- , null SUPP_NO_4
		  /*,(select aps2.segment1 from ap_suppliers aps2 where aps2.vendor_id = :P_SUPP_1) SUPP_NO_1
		  ,(select aps3.segment1 from ap_suppliers aps3 where aps3.vendor_id = :P_SUPP_2) SUPP_NO_2
		  ,(select aps4.segment1 from ap_suppliers aps4 where aps4.vendor_id = :P_SUPP_3) SUPP_NO_3
		  ,(select aps5.segment1 from ap_suppliers aps5 where aps5.vendor_id = :P_SUPP_4) SUPP_NO_4*/
		  ,:P_REP_TYPE REPO_TYPE
		  ,TO_DATE(:P_FROM_DOC_DATE,'RRRR/MM/DD') DOC_FROM_DATE
		  ,to_char(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS'),'DD-MON-YYYY')   DOC_TO_DATE
		  ,to_char(TO_DATE(:P_FROM_GL_DATE,'RRRR/MM/DD HH24:MI:SS'),'DD-MON-YYYY') GL_FROM_DATE
		  ,to_char(TO_DATE(:P_TO_GL_DATE,'RRRR/MM/DD HH24:MI:SS'),'DD-MON-YYYY')   GL_TO_DATE
	FROM  ap_suppliers aps
		  ,ap_suppliers aps1
	WHERE aps.vendor_id = :P_FROM_SUPP_NO
	  AND aps1.vendor_id = :P_TO_SUPP_NO
]]>
</sqlStatement> 
<sqlStatement name="Q_1">
<![CDATA[ 
SELECT asup.segment1              vendor_number
      ,asup.vendor_name           vendor_name
      ,asup.vendor_name_alt       vendor_name_alt
      ,asup.vendor_id             vendor_id   
      ,ass.vendor_site_id         vendor_site_id 
      ,ass.vendor_site_code       vendor_site_code
      ,ass.vendor_site_code_alt   vendor_site_code_alt
      ,ass.address_line1 ||' , '||
       ass.address_line2 ||' , '||
       ass.address_line3 ||' , '||
       ass.city ||' , '||
       ass.state ||' , '||
       ass.zip                    vendor_site_address
      ,ass.vat_registration_num   vat_registration_num
      ,hro.name                   organization_name
      ,hro.organization_id        organization_id
      --,XXOAAPSUPPSTMT.balance_brought_forward(ass.vendor_id,ass.vendor_site_id,ass.org_id) balance_brought_forward
      ,NVL(
			DECODE(:p_from_gl_date,NULL,0,	(
					select sum(acctd_rounded_cr) - sum(acctd_rounded_dr) 
					  from xla_trial_balances 
					  where party_id = asup.vendor_id
                      AND ledger_id  IN (SELECT DISTINCT SET_OF_BOOKS_ID FROM ORG_ORGANIZATION_DEFINITIONS
                      WHERE OPERATING_UNIT = fnd_profile.VALUE ('ORG_ID') AND ROWNUM =1)
					  and gl_date <= trunc(to_date(:p_from_gl_date,'RRRR/MM/DD HH24:MI:SS') - 1))
				)
			,0
			)balance_brought_forward
FROM   ap_suppliers                 asup
      ,ap_supplier_sites_all        ass
      ,hr_operating_units           hro
WHERE  asup.vendor_id = ass.vendor_id
  AND  ass.org_id = hro.organization_id
  AND  asup.enabled_flag = 'Y'
  AND ass.org_id = fnd_profile.VALUE ('ORG_ID')
  AND  asup.vendor_id between  nvl(:P_FROM_SUPP_NO,asup.vendor_id) and nvl(:P_TO_SUPP_NO,asup.vendor_id)
/* AND  (asup.vendor_name BETWEEN :P_FROM_SUPP_NAME AND :P_TO_SUPP_NAME
   OR (((asup.vendor_id IN (:P_SUPP_1,:P_SUPP_2,:P_SUPP_3,:P_SUPP_4)) OR
         (asup.vendor_id BETWEEN :P_FROM_SUPP_NO AND :P_TO_SUPP_NO)) OR
         asup.vendor_id = (DECODE(:P_SUPP_1||:P_SUPP_2||:P_SUPP_3||:P_SUPP_4||:P_FROM_SUPP_NO||:P_TO_SUPP_NO
                                            ,NULL,asup.vendor_id))))*/
  &gc_reporting_entity
  &gc_vend_type
  &gc_pay_group
]]>
</sqlStatement>
<sqlStatement name="Q_MAIN">
<![CDATA[ 
SELECT
	qry.*,
	(qry.entered_amount * qry.exch_rate) accounted_amount
FROM
	(
		SELECT 'I'                                         transaction_type
				,alc.displayed_field                         lookup_value
				,ai.invoice_type_lookup_code                 lookup_code
				,ai.invoice_num                              doc_number
			  ,to_char(ai.voucher_num)					   voucher_num
			  ,ai.doc_sequence_value					   doc_seq
				,ai.invoice_date+1                           doc_date
				,ai.payment_status_flag                      payment_status_flag
				,ai.gl_date+1                                gl_date
				,ai.invoice_currency_code                    currency_code
				--,ai.invoice_amount * NVL(ai.exchange_rate,1) accounted_amount
				--,ai.invoice_amount                           entered_amount
				,ai.invoice_amount                            invoice_amount
				,((
					NVL
							  (
								(
									SELECT   (  
													NVL (ai.invoice_amount, 0)
													- 
													SUM (NVL (aip.AMOUNT, 0))
												 ) unpaid_amount
										  FROM 
												 apps.ap_invoice_payments_all aip,
												 ap_checks_all aca
										 WHERE 1 =1
											AND aip.invoice_id = ai.invoice_id
											AND aip.check_id = aca.check_id
											AND trunc(aca.check_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aca.check_date))
											AND trunc(aip.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aip.accounting_date))
								 ),
								ai.invoice_amount
							  ) 
							  - 
								NVL(
									  (
										 SELECT  (0 - SUM(NVL(aid.amount,0)))
										 FROM ap_invoice_distributions_all aid,
										  ap_invoice_lines_all         ail
										  WHERE ail.invoice_id = ai.invoice_id
										  AND   ail.invoice_id = aid.invoice_id
										  AND   ail.line_number = aid.invoice_line_number
										  AND   aid.line_type_lookup_code = 'PREPAY'
										  AND   aid.prepay_distribution_id IS NOT NULL
										  AND   NVL(ail.invoice_includes_prepay_flag, 'N') = 'N'
										  AND   trunc(aid.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aid.accounting_date))
									  )
									  ,0
									)
				) - 
				NVL(
									  (
										 SELECT  (SUM(NVL(aid.amount,0)))
										 FROM ap_invoice_distributions_all aid,
										  ap_invoice_lines_all         ail
										  WHERE ail.invoice_id = ai.invoice_id
										  AND   ail.invoice_id = aid.invoice_id
										  AND   ail.line_number = aid.invoice_line_number
										  AND   aid.line_type_lookup_code = 'ITEM'
										  AND   aid.POSTED_FLAG = 'N'
										  AND   aid.ACCOUNTING_EVENT_ID is null
										  AND   trunc(aid.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aid.accounting_date))
									  )
									  ,0
									)) entered_amount
				,nvl(ai.exchange_rate,1) exch_rate
				,ai.description                              description
				,gp.period_name                              gp_period_name
				,gp.period_num                               gp_period_num
				,gp.start_date                               gp_start_date
				,AP_INVOICES_PKG.get_posting_status (ai.invoice_id) posting_status
				,AP_INVOICES_PKG.get_wfapproval_status(ai.invoice_id,ai.org_id) approval_status
				,XXOAAPSUPPSTMT.invoice_validate_status(ai.invoice_id) validate_status
			  --,XXOAAPSUPPSTMT.payment_ref_no(ai.invoice_id) payment_refer_no
			  ,(
			  		  SELECT NVL(aia.invoice_num,'A')
					    FROM ap_invoices_all aia,
					         ap_invoice_lines_all ail,
					         ap_invoice_distributions_all aid
					   WHERE aia.invoice_id = ail.prepay_invoice_id
					     AND ail.invoice_id = ai.invoice_id
					     AND   aid.line_type_lookup_code = 'PREPAY'
					     AND   ail.invoice_id = aid.invoice_id
					     AND   ail.line_number = aid.invoice_line_number
						  AND   aid.prepay_distribution_id IS NOT NULL
						  AND   NVL(ail.invoice_includes_prepay_flag, 'N') = 'N'
						  AND   trunc(aid.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aid.accounting_date))
					     AND ROWNUM = 1
			  	) 
			  	||
			  	(
					SELECT   aca.doc_sequence_value
						  FROM 
								 apps.ap_invoice_payments_all aip,
								 ap_checks_all aca
						 WHERE 1 =1
							AND aip.invoice_id = ai.invoice_id
							AND aip.check_id = aca.check_id
							AND trunc(aca.check_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aca.check_date))
							AND trunc(aip.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aip.accounting_date))
							and (aca.void_date is null OR (trunc(aca.void_date) > nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aca.void_date))))
							AND ROWNUM = 1
			  	)
			  	payment_refer_no
			  ,ai.cancelled_date                           cancel_date
			  ,TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS'))+1 sample_dt
		FROM   ap_invoices_all     ai
				,gl_periods      gp
				,gl_ledgers      gled
				,ap_lookup_codes alc
		WHERE  ai.vendor_id                = :vendor_id
		  AND  ai.vendor_id between  nvl(:P_FROM_SUPP_NO,ai.vendor_id) and nvl(:P_TO_SUPP_NO,ai.vendor_id)
		  AND  ai.vendor_site_id           = :vendor_site_id
		  AND trunc(ai.INVOICE_DATE) <= nvl(TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(ai.INVOICE_DATE))
		  AND  ( ai.invoice_id in 
            (select invoice_id from apps.ap_invoice_payments_all 
			where trunc(accounting_date) >= nvl(TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(accounting_date)))
		  OR  ai.PAYMENT_STATUS_FLAG not in ('Y'))
		  AND  gled.period_set_name        = gp.period_set_name
		  AND  gled.accounted_period_type  = gp.period_type
		  AND  gp.adjustment_period_flag   ='N'
		  AND  AP_INVOICES_PKG.GET_POSTING_STATUS( ai.INVOICE_ID) in ('Y','P')
		  AND  gled.ledger_id              = ai.set_of_books_id
		  AND  ai.invoice_type_lookup_code = alc.lookup_code
		  AND  ai.invoice_type_lookup_code <> 'PREPAYMENT'
		  AND  alc.lookup_type             = 'INVOICE TYPE'
		  AND  ai.gl_date      BETWEEN gp.start_date AND gp.end_date
		  AND  trunc(ai.gl_date) <= nvl(TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(ai.gl_date))
		  AND  ai.gl_date BETWEEN nvl(TO_DATE(:P_FROM_GL_DATE,'RRRR/MM/DD HH24:MI:SS'),ai.gl_date) 
									 AND nvl(TO_DATE(:P_TO_GL_DATE,'RRRR/MM/DD HH24:MI:SS')+1,ai.gl_date)
		  AND ai.org_id = fnd_profile.VALUE ('ORG_ID')
		  AND  AP_INVOICES_PKG.get_posting_status(ai.invoice_id) 
										= DECODE(:P_ACCOUNTED,'ACCOUNTED','Y'
																	,'UNACCOUNTED','N'
																	,AP_INVOICES_PKG.get_posting_status(ai.invoice_id))
		AND ap_invoices_pkg.get_approval_status (ai.invoice_id, ai.invoice_amount, ai.payment_status_flag, ai.invoice_type_lookup_code) IN ('UNPAID','NEEDS REAPPROVAL', 'APPROVED','PERMANENT','FULL')
		AND ai.wfapproval_status IN ('WFAPPROVED', 'MANUALLY APPROVED','NOT REQUIRED','REQUIRED','REJECTED')
		AND NVL( (		 SELECT  (SUM(NVL(aid.amount,0)))
															 FROM ap_invoice_distributions_all aid,
															  ap_invoice_lines_all         ail
															  WHERE ail.invoice_id = ai.invoice_id
															  AND   ail.invoice_id = aid.invoice_id
															  AND   ail.line_number = aid.invoice_line_number
															  --commented and added start below for the SR#241834
															 --  AND   trunc(aid.accounting_date) <= trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS'))
															  AND   trunc(aid.accounting_date) <= NVL(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aid.accounting_date))
															  ----added End above for the SR#241834
														  )
														  ,0
														)  <> 0
		  AND (
				  nvl(:P_REP_TYPE,'XX') != 'Outstanding' 
				  OR
				  (
						nvl(:P_REP_TYPE,'XX') = 'Outstanding' 
						AND    
									(
										NVL
												  (
													(
														SELECT   (  
																		NVL (ai.invoice_amount, 0)
																		- 
																		SUM (NVL (aip.AMOUNT, 0))
																	 ) unpaid_amount
															  FROM 
																	 apps.ap_invoice_payments_all aip,
																	 ap_checks_all aca
															 WHERE 1 =1
																AND aip.invoice_id = ai.invoice_id
																AND aip.check_id = aca.check_id
																AND trunc(aca.check_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aca.check_date))
																AND trunc(aip.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aip.accounting_date))
													 ),
													ai.invoice_amount
												  ) 
												  - 
													NVL(
														  (
															 SELECT  (0 - SUM(NVL(aid.amount,0)))
															 FROM ap_invoice_distributions_all aid,
															  ap_invoice_lines_all         ail
															  WHERE ail.invoice_id = ai.invoice_id
															  AND   ail.invoice_id = aid.invoice_id
															  AND   ail.line_number = aid.invoice_line_number
															  AND   aid.line_type_lookup_code = 'PREPAY'
															  AND   aid.prepay_distribution_id IS NOT NULL
															  AND   NVL(ail.invoice_includes_prepay_flag, 'N') = 'N'
															  AND   trunc(aid.accounting_date) <= nvl(trunc(to_date(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aid.accounting_date))
														  )
														  ,0
														)
								) * 
								nvl(ai.exchange_rate,1) 
						 <> 0
				  ) 
				)
	) qry
ORDER BY 
	gp_period_name, description
]]>
  </sqlStatement>
  <sqlStatement name="Q_MAIN1">
<![CDATA[ 
SELECT 'P'                                   transaction_type
      --,alc.displayed_field                 lookup_value
	  ,'Prepayment'                   		 lookup_value
      ,ac.payment_method_lookup_code         lookup_code
      ,ai.invoice_num 						 doc_number
	  ,to_char(ac.check_voucher_num)		 voucher_num
	  ,ac.doc_sequence_value				 doc_seq
      ,ac.check_date+1                         doc_date
      ,NULL                                  payment_status_flag
      ,aip.accounting_date+1                   gl_date
      ,ac.currency_code                      currency_code
	  ,ai.invoice_amount                     invoice_amount  ----added for the SR#429062
      ,NVL(
				(aip.amount - 
					 (SELECT SUM(total_dist_amount -
								NVL(prepay_amount_remaining, total_dist_amount)) applied_amt
						 FROM ap_invoice_distributions_all aid,
								ap_invoice_lines_all ail
						WHERE aid.invoice_id = ai.invoice_id 
						  AND aid.invoice_id = ail.invoice_id
						  AND aid.invoice_line_number = ail.line_number
						  AND ail.line_type_lookup_code <> 'TAX'
						  AND aid.line_type_lookup_code IN ('ITEM', 'ACCRUAL', 'REC_TAX', 'NONREC_TAX')
						  AND NVL(reversal_flag,'N') <> 'Y'
					 )
				 ) 
				 * 
				 NVL(aip.exchange_rate,1) 
			,0) accounted_amount
      ,NVL(
				(aip.amount  -
					 (SELECT SUM(total_dist_amount -
								NVL(prepay_amount_remaining, total_dist_amount)) applied_amt
						 FROM ap_invoice_distributions_all aid,
								ap_invoice_lines_all ail
						WHERE aid.invoice_id = ai.invoice_id 
						  AND aid.invoice_id = ail.invoice_id
						  AND aid.invoice_line_number = ail.line_number
						  AND ail.line_type_lookup_code <> 'TAX'
						  AND aid.line_type_lookup_code IN ('ITEM', 'ACCRUAL', 'REC_TAX', 'NONREC_TAX')
						  AND NVL(reversal_flag,'N') <> 'Y'
					 )
				 ) 
			,0)entered_amount
      ,ai.description                        description
      ,gp.period_name                        gp_period_name
      ,gp.period_num                         gp_period_num
      ,gp.start_date                         gp_start_date
      ,aip.accrual_posted_flag               posting_status
      ,alc1.displayed_field                  approval_status
      ,NULL                                  validate_status
	  ,NULL                                  cancel_date
	  ,ac.check_number						 payment_refer_no
FROM   ap_invoices_all         ai
      ,ap_invoice_payments_all aip
      ,ap_checks_all           ac
      ,gl_periods          gp
      ,gl_ledgers          gled
      ,ap_lookup_codes     alc
      ,ap_lookup_codes     alc1
WHERE  aip.check_id               = ac.check_id
  AND  ac.vendor_id               = :vendor_id
  AND  ac.vendor_site_id          = :vendor_site_id
  AND  ai.vendor_id between  nvl(:P_FROM_SUPP_NO,ai.vendor_id) and nvl(:P_TO_SUPP_NO,ai.vendor_id)
  AND trunc(ai.INVOICE_DATE) <= nvl(TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(ai.INVOICE_DATE))
  AND  ai.invoice_id in 
          (select aip.invoice_id from apps.ap_invoice_payments_all aip
          where invoice_id = ai.invoice_id 
          AND  trunc(aip.accounting_date) >= nvl(TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(aip.accounting_date)))
  AND  aip.invoice_id             = ai.invoice_id
  AND  gled.period_set_name       = gp.period_set_name
  AND  gled.accounted_period_type = gp.period_type
  AND  gp.adjustment_period_flag  ='N'
  AND  gled.ledger_id             = aip.set_of_books_id
  AND  ac.payment_type_flag       = alc.lookup_code
  AND  alc.lookup_type            = 'PAYMENT TYPE'
  AND  alc1.lookup_type           = 'CHECK STATE'
  AND  ai.invoice_type_lookup_code = 'PREPAYMENT'
  AND  ac.status_lookup_code      = alc1.lookup_code
  AND  aip.accounting_date BETWEEN gp.start_date AND gp.end_date
  AND  ai.org_id = fnd_profile.VALUE ('ORG_ID')
  AND  trunc(ac.check_date)  <= nvl(TRUNC(TO_DATE(:P_TO_DOC_DATE,'RRRR/MM/DD HH24:MI:SS')),trunc(ac.check_date))
  AND  aip.accounting_date BETWEEN nvl(TO_DATE(:P_FROM_GL_DATE,'RRRR/MM/DD HH24:MI:SS'),aip.accounting_date) AND 
  											  nvl(TO_DATE(:P_TO_GL_DATE,'RRRR/MM/DD HH24:MI:SS')+1,aip.accounting_date)
  AND nvl(ap_invoices_pkg.get_approval_status(ai.invoice_id, ai.invoice_amount, ai.payment_status_flag,ai.invoice_type_lookup_code),'XX') != 'FULL'
  --AND (
  --      nvl(:P_REP_TYPE,'XX') != 'Outstanding' 
  --      OR
  --      (
  --          nvl(:P_REP_TYPE,'XX') = 'Outstanding' AND nvl(ai.invoice_amount,0) != nvl(ai.amount_paid,0)
  --      ) 
  --    )
  &gc_pmt_accounted
  &gc_pmt_org_id
  &gc_pmt_currency	
ORDER BY gp_period_name, ai.description

]]>
  </sqlStatement>
</dataQuery>
<dataTrigger name="beforeReport" source="XXOAAPSUPPSTMT.beforereport"/>
<dataStructure>
  <group name="G_DATE"   source="Q_DATE">
    <element name="SUPP_NO_FROM"   dataType="VARCHAR2" value="SUPP_NO_FROM"/>
	<element name="SUPP_NO_TO" 	   dataType="VARCHAR2" value="SUPP_NO_TO"/>
	--<element name="SUPP_NAME_FROM" dataType="VARCHAR2" value="SUPP_NAME_FROM"/>
	---<element name="SUPP_NAME_TO"   dataType="VARCHAR2" value="SUPP_NAME_TO"/>
	--<element name="SUPP_NO_1" 	   dataType="VARCHAR2" value="SUPP_NO_1"/>
	--<element name="SUPP_NO_2" 	   dataType="VARCHAR2" value="SUPP_NO_2"/>
	--<element name="SUPP_NO_3" 	   dataType="VARCHAR2" value="SUPP_NO_3"/>
	--<element name="SUPP_NO_4" 	   dataType="VARCHAR2" value="SUPP_NO_4"/>
	<element name="REPO_TYPE" 	   dataType="VARCHAR2" value="REPO_TYPE"/>
	<element name="DOC_FROM_DATE"  dataType="DATE" value="DOC_FROM_DATE"/>
    <element name="DOC_TO_DATE"    dataType="DATE" value="DOC_TO_DATE"/>
    <element name="GL_FROM_DATE"   dataType="DATE" value="GL_FROM_DATE"/>
